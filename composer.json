{
    "name": "maleficarum/api",
    "description": "API component",
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-PDO": "*",
        "ext-phalcon": ">=3.0",
        "php-amqplib/php-amqplib": "2.6.3",
        "maleficarum/ioc": "^1.0",
        "maleficarum/environment": "^1.0",
        "maleficarum/config": "^1.0",
        "maleficarum/profiler": "^1.0",
        "maleficarum/request": "^1.0",
        "maleficarum/response": "^1.0",
        "maleficarum/handler": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Maleficarum\\Api\\": "src/Api/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maleficarum\\Api\\Test\\": "test/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:pharaun13/maleficarum-ioc.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pharaun13/maleficarum-environment.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pharaun13/maleficarum-config.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pharaun13/maleficarum-profiler.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pharaun13/maleficarum-request.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pharaun13/maleficarum-response.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:pharaun13/maleficarum-handler.git"
        }
    ]
}

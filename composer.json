{
    "name": "maleficarum/api",
    "description": "API component",
    "require": {
        "php": ">=7.0",
        "ext-mbstring": "*",
        "ext-PDO": "*",
        "ext-phalcon": ">=3.0",
        "maleficarum/ioc": "^2.0",
        "maleficarum/environment": "^3.0",
        "maleficarum/config": "^3.0",
        "maleficarum/request": "^3.0",
        "maleficarum/http-response": "^3.0",
        "maleficarum/handler": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Maleficarum\\Api\\": "src/Api/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Maleficarum\\Api\\Test\\": "test/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
